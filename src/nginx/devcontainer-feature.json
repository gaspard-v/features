{
    "id": "nginx",
    "version": "1.0.3",
    "name": "nginx",
    "documentationURL": "https://github.com/gaspard-v/features/tree/main/src/nginx",
    "options": {
        "version": {
            "type": "string",
            "enum": [
                "mainline",
                "stable"
            ],
            "default": "mainline",
            "description": "Select or enter a nginx version"
        },
        "enablePhpFpm": {
            "type": "boolean",
            "default": true,
            "description": "configure php-fpm websites support"
        }
    },
    "customizations": {},
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils",
        "ghcr.io/gaspard-v/features/php-fpm"
    ]
}
